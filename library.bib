isbn = {9781424442416},
issn = {1944-9925},
journal = {2009 IEEE Power Energy Soc. Gen. Meet. PES '09},
keywords = {Adaptive network zone division,Control center,Field site application,Secondary voltage control,Tertiary voltage control},
pages = {1--5},
title = {{Development and applications of system-wide automatic voltage control system in China}},
year = {2009}
}
