journal = {IEEE Trans. Smart Grid},
keywords = {Load aggregation,V2G,distributed generation,electric vehicles,mobility,plug-in electric vehicles,renewable energy sources,stochastic security-constrained unit commitment},
mendeley-tags = {distributed generation,electric vehicles,mobility},
number = {3},
pages = {1271--1279},
title = {{Hourly coordination of electric vehicle operation and volatile wind power generation in SCUC}},
volume = {3},
