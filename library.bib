publisher = {Elsevier B.V.},
title = {{A new error measure for forecasts of household-level, high resolution electrical energy consumption}},
url = {http://dx.doi.org/10.1016/j.ijforecast.2013.08.002},
volume = {30},
year = {2014}
}
