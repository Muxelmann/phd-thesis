issn = {03062619},
journal = {Appl. Energy},
keywords = {Battery,Control,Energy storage,LV networks,Smart grid},
pages = {202--213},
publisher = {Elsevier Ltd},
title = {{The impact of location and type on the performance of low-voltage network connected battery energy storage systems}},
url = {http://dx.doi.org/10.1016/j.apenergy.2015.12.045},
