journal = {IEEE Trans. Power Syst.},
keywords = {Active Network Management (ANM),dynamic optimal power flow (DOPF),energy storage,flexible demand,optimal power flow (OPF)},
number = {1},
pages = {121--131},
title = {{Dynamic optimal power flow for active distribution networks}},
volume = {29},
year = {2014}
}
