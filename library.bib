keywords = {device aging,electrical energy storage,milp,mixed integer linear program,peak shaving,polynomial time optimal algorithm},
pages = {465},
title = {{Scheduling of Electricity Storage for Peak Shaving with Minimal Device Wear}},
url = {http://www.mdpi.com/1996-1073/9/6/465},
volume = {9},
year = {2016}
}
