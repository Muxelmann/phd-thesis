journal = {IEEE Trans. Power Deliv.},
keywords = {Active power filters,CPC,Currents' physical components,P-q theory,Power theory,Powers,Switching compensators,Unbalanced systems},
number = {1},
pages = {362--367},
title = {{Instantaneous reactive power p-q theory and power properties of three-phase systems}},
volume = {21},
year = {2006}
