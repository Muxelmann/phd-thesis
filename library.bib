isbn = {0960-1481},
issn = {09601481},
journal = {Renew. Energy},
keywords = {Artificial neural networks,Daily energy output,Large solar thermal systems,Maximum temperature of storage tank},
pages = {90--97},
publisher = {Elsevier Ltd},
title = {{Artificial neural networks for the performance prediction of large solar systems}},
url = {http://dx.doi.org/10.1016/j.renene.2013.08.049},
