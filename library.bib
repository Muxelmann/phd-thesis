keywords = {Optimal energy delivery,Plug-in hybrid electric vehicle,Stochastic inventory theory,Vehicle-to-grid,electric vehicles},
mendeley-tags = {electric vehicles},
number = {4},
pages = {2230--2243},
title = {{Optimizing the energy delivery via V2G systems based on stochastic inventory theory}},
volume = {4},
year = {2013}
}
